<pre>
  <% @articles.each do |article| %>
    @article{<%= article.key %>,
      author = {<%= article.author %>},
      title = {<%= article.title %>},
      journal = {<%= article.journal %>},
      year = {<%= article.year %>},
    }

  <% end %>
  <% @books.each do |book| %>
    @book{<%= book.key %>,
      author = {<%= book.author %>},
      title = {<%= book.title %>},
      publisher = {<%= book.publisher %>},
      year = {<%= book.year %>},
    }

  <% end %>
</pre>